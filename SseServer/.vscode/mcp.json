{
    "servers": {
        "lookup": {
            "command": "python",
            "args": [
                "lookupServer.py"
            ],
            "env": {
                "OPENAI_API_KEY": "<YOUR_OPENAI_API_KEY>"
            }
        },
        "brave-search": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-brave-search",
            ],
            "env": {
                "BRAVE_API_KEY": ""
            }
        }
    }
}