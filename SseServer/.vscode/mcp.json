{
    "servers": {
        "vectordb-lookup": {
            "command": "python",
            "args": [
                "-u",
                "vectordb_lookup_server.py",
                "--host",
                "0.0.0.0"
            ],
            "env": {
                "OPENAI_API_KEY": "<YOUR_OPENAI_API_KEY>"
            }
        },
    }
}