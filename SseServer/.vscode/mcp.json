{
    "servers": {
        "StockSentiment": {
            "command": "python",
            "args": [
                "mcpSECServer.py"
            ],
            "env": {
                "OPENAI_API_KEY": "<YOUR_OPENAI_API_KEY>"
            }
        },
        "RAGLookup": {
            "command": "python",
            "args": [
                "mcpRAGServer.py"
            ],
            "env": {
                "OPENAI_API_KEY": "<YOUR_OPENAI_API_KEY>"
            }
        },
        "brave-search": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-brave-search",
                "--stdio"
            ],
            "env": {
                "BRAVE_API_KEY": ""
            }
        },
        "memory": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-memory",
                "--persist",
                "--file",
                "memory.json"
            ]
        },
    }
}